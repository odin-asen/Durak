<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<VBox maxWidth="1.7976931348623157E308" xmlns:fx="http://javafx.com/fxml">
  <HBox maxWidth="-1.0" VBox.vgrow="NEVER">
    <ImageView fx:id="message_icon" />
    <Label fx:id="messageContainer" />
  </HBox>
  <HBox maxWidth="-1.0" VBox.vgrow="NEVER">
    <fx:define>
      <Insets fx:id="defInset" bottom="2" top="2" left="4" right="4"/>
    </fx:define>
    <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS"/>
    <Button fx:id="dialogOkayButton" text="%button.okay" HBox.margin="$defInset"/>
    <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS"/>
  </HBox>
</VBox>
